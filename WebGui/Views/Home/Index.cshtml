@{
    @using WebGui.Models;
    @model IEnumerable<Customer>
}
<div id="tableView">
    <table>

        @foreach(var customer in Model)
        {
            <tr>
				<td>@customer.index</td>
				<td>@customer.acctNo</td>
				<td>@customer.pin</td>
				<td>@customer.balance</td>
				<td>@customer.firstname</td>
				<td>@customer.lastname</td>
			</tr>
        }
    </table>
</div>
